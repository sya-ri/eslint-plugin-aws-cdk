{
  "name": "eslint-plugin-aws-cdk",
  "version": "0.1.0",
  "description": "ESLint plugin for aws-cdk",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "aws-cdk"
  ],
  "homepage": "https://github.com/sya-ri/eslint-plugin-aws-cdk#readme",
  "bugs": {
    "url": "https://github.com/sya-ri/eslint-plugin-aws-cdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sya-ri/eslint-plugin-aws-cdk.git"
  },
  "license": "MIT",
  "author": "sya-ri",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "check": "eslint .",
    "fix": "eslint . --fix",
    "test": "jest"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^6.0.0 || ^7.0.0 || ^8.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/rule-tester": "^8.10.0",
    "aws-cdk-lib": "^2.162.1",
    "constructs": "^10.4.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/tests/**/*(*.)@(spec|test).ts"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
